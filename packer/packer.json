{
  "builders": [{
    "type": "amazon-ebs",
    "region": "us-east-1",
    "source_ami": "ami-eb6b0182",
    "instance_type": "t1.micro",
    "ssh_username": "root",
	"ssh_timeout": "5m",
    "ami_name": "vagrant-base {{.CreateTime}}"
  }],
  "provisioners": [{
	"type": "shell",
	"inline": [
		"yum install -y puppet"
	]
  }],
  "post-processors": ["vagrant"]
}
